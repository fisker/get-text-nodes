{
  "name": "get-text-nodes",
  "version": "1.0.5",
  "description": "get child text nodes",
  "homepage": "https://github.com/fisker/get-text-nodes#readme",
  "bugs": {
    "url": "https://github.com/fisker/get-text-nodes/issues"
  },
  "repository": "fisker/get-text-nodes",
  "funding": "https://github.com/fisker/get-text-nodes?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.min.mjs",
  "browser": "dist/index.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean build test dist",
    "test": "ava",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@commitlint/cli": "9.0.1",
    "@fisker/commitlint-config": "1.2.1",
    "@fisker/eslint-config": "3.0.0",
    "@fisker/eslint-config-ava": "2.0.0",
    "@fisker/husky-config": "3.0.0",
    "@fisker/lint-staged-config": "2.2.0",
    "@fisker/prettier-config": "3.0.0",
    "ava": "3.10.1",
    "cz-conventional-changelog": "3.2.0",
    "dedent": "0.7.0",
    "del-cli": "3.0.1",
    "eslint": "7.4.0",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "jsdom": "16.3.0",
    "lint-staged": "10.2.11",
    "markdownlint-cli": "0.23.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "rollup": "2.21.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-filesize": "9.0.2",
    "rollup-plugin-prettier": "2.1.0",
    "rollup-plugin-terser": "6.1.0",
    "sort-package-json": "1.44.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
